<Window x:Class="Modeling_q_pipeline.View.TextStatisticWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Modeling_q_pipeline.View"
        mc:Ignorable="d"
        Title="TextStatistic" Height="400" Width="350"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        Closing="TextStatistic_OnClosing"
        Icon="Img/folder.png">
    <Grid>
        <ScrollViewer>
            <TextBlock  x:Name="TextStatisticInfo"
                       TextWrapping="Wrap"></TextBlock>
        </ScrollViewer>
    </Grid>
</Window>
